public interface IAreaCalculable {
    double area();
}

public interface IVolumeCalculable {
    double volume();
}

public class Circle implements IAreaCalculable {
    private double radius;

    public Circle(double radius) {
        this.radius = radius;
    }

    @Override
    public double area() {
        return 2 * 3.14 * radius;
    }
}

public class Cube implements IAreaCalculable, IVolumeCalculable {
    private int edge;

    public Cube(int edge) {
        this.edge = edge;
    }

    @Override
    public double area() {
        return 6 * edge * edge;
    }

    @Override
    public double volume() {
        return edge * edge * edge;
    }
}